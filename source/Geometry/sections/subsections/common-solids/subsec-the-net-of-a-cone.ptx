    <text x="175" y="110" font-family="Arial" font-size="12" fill="black">1cm</text>

    <!-- Midpoint of GF (top back edge) -->
    <circle cx="195" cy="45" r="3" fill="none" />
    <text x="198" y="58" font-family="Arial" font-size="12" fill="black">2cm</text>
  </svg>
</body>
</html>

